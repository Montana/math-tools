    "# Takes two ints and returns greatest common denominator with Euclidean Algorithm\n",
    "def gcd(a, b):\n",
    "    if a < b:\n",
    "        # Ensure a is gte than b\n",
    "        return gcd(b, a) \n",
    "    if b == 0: # Covers (0,0) as 0\n",
    "        return a\n",
    "    \n",
    "    return gcd(b, a % b)\n",
    "\n",
    "print(gcd(1071, 462))"
   ]
  }
